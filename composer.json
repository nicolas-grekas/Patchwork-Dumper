{
    "name": "tchwork/debug-bundle",
    "type": "library",
    "description": "The DebugBundle of Symfony 2.6 backported for 2.3+",
    "keywords": ["dump"],
    "homepage": "https://github.com/tchwork/debug-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Grekas",
            "email": "p@tchwork.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.2",
        "symfony/event-dispatcher": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/var-dumper": "~2.6"
    },
    "require-dev": {
        "twig/twig": "~1.12"
    },
    "conflict": {
        "symfony/debug-bundle": ">=2.6"
    },
    "autoload": {
        "psr-0": {
            "Symfony\\Bundle\\DebugBundle\\": "src",
            "Symfony\\Bridge\\Twig\\": "src",
            "Symfony\\Component\\HttpKernel\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
